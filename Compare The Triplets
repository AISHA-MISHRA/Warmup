#include <cstdio>
#include <iostream>
using namespace std;

int main(){
    int arr1[3],arr2[3];
    for(int i=0;i<3;i++){
        cin>>arr1[i];
    }
    int a=0,b=0;
    for(int i=0;i<3;i++){
        cin>>arr2[i];
        if(arr1[i]>arr2[i]) a++;
        else if(arr1[i]<arr2[i]) b++;
    }
    cout<<a<<' '<<b<<endl;
    return 0;
}
